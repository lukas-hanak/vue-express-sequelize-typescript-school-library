{
  "name": "school-library-express-sequelize-typescript-services",
  "version": "1.0.0",
  "author": "Lukas Hanak",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "watch": "ts-node-dev --respawn index.dev.ts",
    "build-ts": "tsc",
    "serve": "node dist/index.js",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:undo": "sequelize db:migrate:undo",
    "db:seed:all": "sequelize-cli db:seed:all",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "body-parser": "~1.19.0",
    "cors": "^2.8.5",
    "dotenv": "~8.2.0",
    "express": "~4.17.3",
    "mysql2": "^2.1.0",
    "reflect-metadata": "~0.1.13",
    "sequelize": "~5.21.2",
    "sequelize-cli": "~5.5.1",
    "sequelize-typescript": "^1.1.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/bluebird": "~3.5.29",
    "@types/node": "~12.12.14",
    "@types/validator": "~12.0.1",
    "module-alias": "~2.2.2",
    "ts-node-dev": "~1.0.0-pre.44",
    "typescript": "~3.7.2"
  }
}
