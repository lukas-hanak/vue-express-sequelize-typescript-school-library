{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 2],
        "interface-name": false,
        "no-consecutive-blank-lines": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "quotemark": [true, "single"],
        "no-trailing-whitespace": false,
        "member-access": false,
        "trailing-comma": true
    }
}
